{
  "name": "pages",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest --passWithNoTests",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format-check": "rome format src",
    "format": "rome format --write src",
    "check": "rome check src",
    "check-fix": "rome check --apply-unsafe src",
    "lint": "yarn format-check && yarn check",
    "lint-fix": "yarn format && yarn check-fix",
    "clean": "rimraf build",
    "analyze": "ANALYZE=true next build",
    "pretest": "yarn build",
    "prebuild": "yarn install",
    "predev": "yarn install",
    "preanalyze": "yarn install",
    "preformat": "yarn install",
    "precheck": "yarn install",
    "preformat-check": "yarn install",
    "precheck-fix": "yarn install",
    "postbuild": "next export",
    "all": "yarn lint && yarn build && yarn test"
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "next": "^13",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13",
    "@swc/jest": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/react": "*",
    "jest": "*",
    "rimraf": "*",
    "rome": "*",
    "swc-loader": "*",
    "typescript": "*"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
