    "predev": "yarn install",
    "preanalyze": "yarn install",
    "preformat": "yarn install",
    "precheck": "yarn install",
    "preformat-check": "yarn install",
    "precheck-fix": "yarn install",
    "prebuild": "yarn lint",
    "all": "yarn build && yarn test"
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/server": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "next": "^13",
    "react": "*",
    "react-dom": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "rimraf": "*",
    "rome": "*",
    "typescript": "*"
  }
