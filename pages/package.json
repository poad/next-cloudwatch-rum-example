{
  "name": "pages",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "export": "next build",
    "start": "next start",
    "format-check": "rome format src",
    "format": "rome format --write src",
    "check": "rome check src",
